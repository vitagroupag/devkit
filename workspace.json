{
  "version": 1,
  "projects": {
    "style-builders": {
      "root": "packages/style-builders",
      "sourceRoot": "packages/style-builders/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/style-builders/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/style-builders/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/style-builders",
            "tsConfig": "packages/style-builders/tsconfig.lib.json",
            "packageJson": "packages/style-builders/package.json",
            "main": "packages/style-builders/src/index.ts",
            "assets": [
              "packages/style-builders/*.md",
              "packages/style-builders/builders.json",
              {
                "glob": "*.json",
                "input": "packages/style-builders/src/lib",
                "output": "src/lib"
              },
              {
                "glob": "*.json",
                "input": "packages/style-builders/src/postcss",
                "output": "src/postcss"
              },
              {
                "glob": "*.json",
                "input": "packages/style-builders/src/sass",
                "output": "src/sass"
              }
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
